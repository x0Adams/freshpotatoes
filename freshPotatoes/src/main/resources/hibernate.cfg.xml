<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="connection.url">jdbc:mariadb://localhost:3306/fresh_potatoes</property>
        <property name="connection.username">root</property>
        <property name="connection.password"></property>
        <property name="connection.driver_class">org.mariadb.jdbc.Driver</property>
        <mapping class="hu.freshpotatoes.pojos.Country"/>
        <mapping class="hu.freshpotatoes.pojos.Genre"/>
        <mapping class="hu.freshpotatoes.pojos.GenreMovie"/>
        <mapping class="hu.freshpotatoes.pojos.Movie"/>
        <mapping class="hu.freshpotatoes.pojos.ProductionsCountry"/>
        <mapping class="hu.freshpotatoes.pojos.Rate"/>
        <mapping class="hu.freshpotatoes.pojos.RefreshToken"/>
        <mapping class="hu.freshpotatoes.pojos.Role"/>
        <mapping class="hu.freshpotatoes.pojos.Staff"/>
        <mapping class="hu.freshpotatoes.pojos.StaffToleInMovie"/>
        <mapping class="hu.freshpotatoes.pojos.User"/>
    </session-factory>
</hibernate-configuration>